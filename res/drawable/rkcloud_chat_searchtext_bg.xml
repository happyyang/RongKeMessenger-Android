<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="false">
        <shape android:shape="rectangle" >
            <!-- 填充的颜色 -->
            <solid android:color="@color/white"></solid>
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="10dip" />
        </shape>
    </item>

    <item android:state_pressed="true">
        <shape android:shape="rectangle" >
            <!-- 填充的颜色 -->
            <solid android:color="@color/white"></solid>
            <!-- 设置按钮的四个角为弧形 -->
            <!-- android:radius 弧形的半径 -->
            <corners android:radius="10dip" />
        </shape>
    </item>

    <!--<item android:drawable="@color/rkcloud_chat_btn_bgcolor_disable" android:state_enabled="false"/>
    <item android:drawable="@color/rkcloud_chat_btn_bgcolor_pressed" android:state_pressed="true"/>-->
    <!--<item android:drawable="@color/rkcloud_chat_btn_bgcolor_normal"/>-->

</selector>